      Font.Name = 'Tahoma'
      Font.Style = []
      Lines.Strings = (
        
          'This is a demo of the possible ways for letting a user reorder a' +
          ' list view.'
        ''
        'There are three possible ways:'
        
          'Drag & Drop (Multiple items can be selected using [Ctrl]+[left c' +
          'lick]).'
        'Clicking Buttons (Up/Down) '
        'Shortcuts ([Ctrl]+[Up arrow]/ [Ctrl]+[Down arrow])'
        ''
        
          'Items can be renamed either by pressing [F2] or by double-clicki' +
          'ng.'
        ''
        
          'If you'#39'd like we can implement this together in ghostmaker (it s' +
          'houldn'#39't take long:)'
        'Ajasja')
      ParentFont = False
      TabOrder = 0
    end
